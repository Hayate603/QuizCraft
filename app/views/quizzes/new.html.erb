<h1>新しいクイズを作成</h1>
<%= form_with model: @quiz, local: true do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div>
    <%= form.label :description %>
    <%= form.text_area :description %>
  </div>
  <div>
    <%= form.submit 'クイズを作成' %>
  </div>
<% end %>
